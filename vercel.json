{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "MONGODB_URI": "mongodb://<username>:<password>@<your-mongo-db-host>:27017/viraj"
    }
  }
  